{
	"name": "@xats/eslint-config",
	"type": "module",
	"version": "3.0.2",
	"packageManager": "pnpm@10.7.0",
	"description": "üßÉ Xat's ESLint config preset / Xat Ëá™Áî®Âº∫Ëø´Áóá ESLint ÈÖçÁΩÆ",
	"author": "Xat <i@xat.sh> (https://xat.sh)",
	"license": "MIT",
	"homepage": "https://github.com/withxat/eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/withxat/eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/withxat/eslint-config/issues"
	},
	"keywords": [
		"eslint-config"
	],
	"exports": {
		".": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": "./bin/index.js",
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "nr gen && tsup --clean --dts",
		"stub": "tsup",
		"dev": "npx @eslint/config-inspector --config eslint.config.js",
		"build:inspector": "pnpm build && npx @eslint/config-inspector build",
		"watch": "tsup --watch",
		"lint": "eslint",
		"gen": "tsx scripts/typegen.ts && tsx scripts/versiongen.ts",
		"prepack": "nr build",
		"release": "bumpp && pnpm publish",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"eslint": "catalog:peer"
	},
	"dependencies": {
		"@antfu/install-pkg": "catalog:prod",
